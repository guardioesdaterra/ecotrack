{
  "version": 2,
  "routes": [
    {
      "src": "/grid-overlay.png",
      "dest": "/public/grid-overlay.png"
    },
    {
      "src": "/scanline.png",
      "dest": "/public/scanline.png"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1920, 2048, 3840],
    "domains": ["unpkg.com"],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "unpkg.com"
      }
    ]
  }
} 